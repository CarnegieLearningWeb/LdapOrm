{
    "name": "carnegielearning/ldap-orm-bundle",
    "type": "symfony-bundle",
    "description": "Another LDAP ORM for Symfony2",
    "keywords": ["ucsf", "orm", "ldap", "carnegie learning"],
    "license": "GPL",
    "version": "0.1.0",
    "authors": [
        {
            "name": "UCSF IT Identity & Access Management",
            "email": "iam-team@ucsf.edu"
        },
        {
            "name": "Ronald Chaplin",
            "email": "rchaplin@t73.biz"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/http-kernel": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/class-loader": "^3.0",
        "symfony/yaml": "^3.0",
        "symfony/monolog-bundle": "^2.9",
        "symfony/property-info": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": { "CarnegieLearning\\LdapOrmBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "CarnegieLearning/LdapOrmBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
